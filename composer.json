{
    "name": "findyouractivity/laravel-userstamps",
    "description": "Package to add created_by, updated_by and deleted_by to models.",
    "keywords": ["laravel", "created", "updated", "deleted", "userstamps"],
    "homepage": "https://github.com/findyouractivity/laravel-userstamps",
    "license": "MIT",
    "authors": [
        {
            "name": "Sqits",
            "email": "info@sqits.nl",
            "homepage": "https://www.sqits.nl",
            "role": "Company"
        },
        {
            "name": "Ruud Schaaphuizen",
            "email": "r.schaaphuizen@sqits.nl",
            "homepage": "https://www.sqits.nl",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sqits/laravel-userstamps"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^7.2",
        "larapack/dd": "^1.0",
        "doctrine/dbal": "^2.6"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Sqits\\UserStamps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sqits\\UserStamps\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sqits\\UserStamps\\UserStampsServiceProvider"
            ]
        }
    }
}
